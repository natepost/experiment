<p id="notice"><%= notice %></p>
<h1>Experiment Index Placeholder</h1>

<%= link_to 'New Experiment', new_experiment_path %>
<hr>
  <div class="card-deck">
   <% @experiments.reverse.each do |experiment| %>
     <div class="card">
       <div class="card-block">
         <h4 class="card-title"><%= experiment.title %></h4>
         <p class="card-text"><%= experiment.body %></p>
         <p class="card-text"><small class="text-muted">Last updated <%= time_ago_in_words(experiment.created_at) %> ago</small></p>
         <a href="#" class="btn btn-primary">View</a>
       </div>
     </div>
   <% end %>
</div>
